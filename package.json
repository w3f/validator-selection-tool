{
  "name": "Algorithm integration",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josepot/alg-integration.git"
  },
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "analyze": "source-map-explorer 'dist/assets/*.js'",
    "format": "prettier --write README.md \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "dev": "vite",
    "local": "VITE_ENV=local npm run dev",
    "build": "tsc --project tsconfig.build.json && vite build",
    "serve": "vite preview",
    "prepare": "husky install",
    "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --env=./custom-test-env.js",
    "test:watch": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --env=./custom-test-env.js --watch",
    "lint": "prettier --check README.md \"src/**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "@polkadot/react-identicon": "^3.0.2",
    "@react-rxjs/core": "^0.10.4",
    "@react-rxjs/utils": "^0.9.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-error-boundary": "^3.1.4",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@jest/globals": "^28.1.0",
    "@json-rpc-tools/provider": "^1.7.6",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^16.11.26",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "lint-staged": "^12.4.3",
    "postcss": "^8.4.14",
    "postcss-focus-visible": "^8.0.2",
    "prettier": "^2.6.2",
    "react-test-renderer": "^18.1.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "rollup-plugin-workbox": "^6.2.0",
    "source-map-explorer": "^2.5.2",
    "tailwindcss": "^3.2.1",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2",
    "vite": "^2.9.9"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "semi": false
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": "prettier --write"
  }
}
